{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "[Dev - Default] Run App",
            "program": "${workspaceRoot}/lib/launchers/main.dev.dart",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "[Dev - Chrome] Run App",
            "program": "${workspaceRoot}/lib/launchers/main.dev.dart",
            "request": "launch",
            "type": "dart",
            "args": ["-d", "chrome", " --web-port", "7357"]
        },
        {
            "name": "[Dev - MacOS] Run App",
            "program": "${workspaceRoot}/lib/launchers/main.dev.dart",
            "request": "launch",
            "type": "dart",
            "args": ["-d", "macos"]
        },
        {
            "name": "[Prod] Run App",
            "program": "${workspaceRoot}/lib/launchers/main.prod.dart",
            "request": "launch",
            "type": "dart"
        }
    ],

}